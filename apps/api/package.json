{
    "name": "@dipcord/api",
    "type": "module",
    "version": "1.0.0",
    "private": "true",
    "imports": {
        "#*": {
            "dev": "./src/*",
            "test": "./src/*",
            "default": "./dist/*"
        }
    },
    "scripts": {
        "dev": "tsx --watch --env-file=.env src/index.ts",
        "build": "tsc --project tsconfig.production.json",
        "start": "pnpm build && node dist/index.js",
        "lint": "eslint .",
        "check-types": "tsc --noEmit --project tsconfig.production.json",
        "test": "vitest run",
        "test:unit": "vitest run --config vitest.unit.config.mts",
        "test:integration": "vitest run --config vitest.integration.config.mts",
        "test:watch": "vitest",
        "test:watch:unit": "vitest --config vitest.unit.config.mts",
        "test:watch:integration": "vitest --config vitest.integration.config.mts",
        "test:ui": "vitest --ui",
        "test:ui:unit": "vitest --ui --config vitest.unit.config.mts",
        "test:ui:integration": "vitest --ui --config vitest.integration.config.mts",
        "test:coverage": "vitest run --coverage"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.787.0",
        "@aws-sdk/s3-request-presigner": "^3.787.0",
        "@fastify/autoload": "^6.2.0",
        "@fastify/cookie": "^11.0.2",
        "@fastify/cors": "^11.0.0",
        "@fastify/error": "^4.1.0",
        "@fastify/multipart": "^9.0.3",
        "@fastify/oauth2": "^8.1.2",
        "@fastify/sensible": "^6.0.3",
        "@fastify/swagger": "^9.4.2",
        "@fastify/swagger-ui": "^5.2.2",
        "change-case": "^5.4.4",
        "drizzle-orm": "^0.40.0",
        "drizzle-seed": "^0.3.1",
        "fastify": "^5.2.1",
        "fastify-plugin": "^5.0.1",
        "fastify-socket.io": "^5.1.0",
        "fastify-type-provider-zod": "^4.0.2",
        "pg": "^8.13.3",
        "redis": "^4.7.0",
        "socket.io": "^4.8.1",
        "uuid": "^11.1.0",
        "zod": "^3.24.2"
    },
    "optionalDependencies": {
        "bufferutil": "^4.0.9",
        "utf-8-validate": "^6.0.5"
    },
    "devDependencies": {
        "@dipcord/domain": "workspace:*",
        "@dipcord/eslint-config": "workspace:*",
        "@dipcord/schema": "workspace:*",
        "@dipcord/typescript-config": "workspace:*",
        "@testcontainers/postgresql": "^10.20.0",
        "@testcontainers/redis": "^10.22.0",
        "@types/node": "^22.0.0",
        "@types/pg": "^8.11.11",
        "@vitest/coverage-v8": "3.0.8",
        "@vitest/ui": "3.0.8",
        "drizzle-kit": "^0.30.5",
        "eslint": "^9.21.0",
        "eslint-plugin-drizzle": "^0.2.3",
        "eslint-plugin-format": "^1.0.1",
        "pino-pretty": "^13.0.0",
        "testcontainers": "^10.20.0",
        "tsx": "^4.19.3",
        "typescript": "^5.8.2",
        "vitest": "^3.0.8"
    }
}
