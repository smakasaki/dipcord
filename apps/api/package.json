{
    "name": "@dipcord/api",
    "type": "module",
    "version": "1.0.0",
    "private": "true",
    "imports": {
        "#*": {
            "dev": "./src/*",
            "test": "./src/*",
            "default": "./dist/*"
        }
    },
    "scripts": {
        "dev": "tsx --watch src/index.ts",
        "build": "tsc --project tsconfig.production.json",
        "start": "node dist/index.js",
        "lint": "eslint .",
        "check-types": "tsc --noEmit",
        "test": "vitest run",
        "test:ui": "vitest --ui"
    },
    "devDependencies": {
        "@dipcord/eslint-config": "workspace:*",
        "@dipcord/typescript-config": "workspace:*",
        "@types/node": "^22.0.0",
        "@vitest/ui": "3.0.8",
        "eslint": "^9.21.0",
        "eslint-plugin-drizzle": "^0.2.3",
        "eslint-plugin-format": "^1.0.1",
        "tsx": "^4.19.3",
        "typescript": "^5.8.2",
        "vitest": "^3.0.8"
    }
}
